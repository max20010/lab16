#include "pch.h"
#include <iostream>
using namespace std;
int main()
{
	int n,n1,x;
	cin >> n;
	int* m = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> m[i];
	}
	int max = m[0],max_x=0, min = m[0],min_x=0;
	for (int i = 0; i < n; i++) {
		if (m[i] > max) {
			max = m[i];
			max_x = i;
		}
		if (m[i] > min) {
			min = m[i];
			min_x = i;
		}
	}
	n1 = n + 2;
	int* m1 = new int[n1];
	m1[0] = 0;
	m1[1] = 0;
	for (int i = 2,j=0; i < n1; i++,j++) {
		m1[i] = m[j];
	}
	for (int i = 0; i < n1; i++) {
		if (i == max + 2) {
			x = m[max + 3];
			m[max + 3] = 0;
			for (int j = max + 4,x1=m[j]; j < n1; j++) {
				x1 = x;

			}
		}
	}
	cout << endl;

	for (int i = 0; i < n1; i++) {
		cout << m1[i] << endl;
	}
	delete[]m;
	delete[]m1;
}
